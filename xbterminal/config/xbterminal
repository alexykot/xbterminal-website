server {
	listen 80;
	server_name xbterminal.devlab.su;
	access_log /var/log/nginx/xbterminal.devlab.su_access.log;
	error_log /var/log/nginx/xbterminal.devlab.su_error.log;

	location / {
		uwsgi_pass	unix:///tmp/xbterminal.devlab.su.sock;
		include	uwsgi_params;
	}

	location /media/ {
		alias	/home/xbterminal/static_root/;
	}

	location /static/ {
		alias	/home/xbterminal/static_root/;
	}
}
